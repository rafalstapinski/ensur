<?xml version='1.0' encoding='utf-8'?>
<widget id="io.ensur.app" version="0.0.1" >
    <name>ensur</name>
    <description>
        App that connects to ensur network for encrypted messaging.
    </description>
    <author email="stapinskirafal@gmail.com" href="http://cordova.io">
        ensur team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
